{% extends "base.html.twig" %}

{% block body %}

    <div class="container mt-2 py-5">
       
        <form class="form-group mt-2 py-5">

            {{ form_start(formVehiculo) }}

            <div class="alert-danger">
                {{ form_errors(formVehiculo) }}
            </div>

            <div class="form-group">
                <div class="form-control my-4">
                    {{ form_row(formVehiculo.Matricula) }}
                </div>
                <div class="form-control my-4">
                    {{ form_row(formVehiculo.Marca) }}
                </div>
                <div class="form-control my-4">
                    {{ form_row(formVehiculo.Modelo) }}
                </div>
                <div class="form-control my-4">
                    {{ form_row(formVehiculo.Year) }}
                </div><div class="form-control my-4">
                    {{ form_row(formVehiculo.Image) }}
                </div>
            </div>

                {{form_widget(formVehiculo)}}
                <button class="btn btn-lg btn-primary btn-block mt-5" type="submit">Enviar</button>
            {{ form_end(formVehiculo) }}
        </form>
    </div>

{% endblock %}